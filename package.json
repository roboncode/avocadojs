    "lodash": "4.17.11",
    "pluralize": "^7.0.0",
    "tangjs": "0.2.3",
    "uniqid": "^5.0.3",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "nyc": "13.1.0",
    "vuepress": "0.14.8"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
