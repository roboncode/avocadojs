{
  "name": "orango",
  "version": "1.0.0-alpha-2.0",
  "description": "ArangoDB Object Modeler for Node.js, Foxx and Web Browsers",
  "main": "./index.js",
  "scripts": {
    "examples" : "node examples",
    "test": "nyc --reporter=html --reporter=text mocha --delay",
    "test-no-coverage": "mocha --delay",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "examples:setup": "cd examples && ./setup.sh",
    "examples:server": "cd examples/server && yarn serve",
    "examples:client": "cd examples/client && yarn serve"
  },
  "author": "Rob Taylor <roboncode@gmail.com> (https://www.linkedin.com/in/roboncode/)",
  "license": "MIT",
  "dependencies": {
    "arangojs": "6.10.0",
    "colors": "1.3.3",
